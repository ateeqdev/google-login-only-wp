msgid "Authorized Users"
msgstr "Авторизовані користувачі"

#: includes/class-otl-admin-settings.php:427
msgid "Add User"
msgstr "Додати користувача"

#: includes/class-otl-admin-settings.php:430
msgid "Email Address"
msgstr "Електронна адреса"

#: includes/class-otl-admin-settings.php:431
